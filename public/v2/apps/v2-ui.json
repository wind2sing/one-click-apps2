{
  "captainVersion": "2",
  "description": "Support multi-protocol multi-user v2ray panel.",
  "displayName": "v2-ui",
  "dockerCompose": {
    "version": "3.3",
    "services": {
      "$$cap_appname": {
        "image": "sola97/v2-ui",
        "containerHttpPort": "65432",
        "restart": "always",
        "volumes": ["$$cap_appname-v2-ui:/etc/v2-ui"]
      }
    }
  },
  "instructions": {
    "start": "V2-ui is a v2ray panel.",
    "end": "V2-ui is deployed and available as $$cap_appname"
  },
  "variables": []
}
